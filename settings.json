{
  "security.workspace.trust.untrustedFiles": "open",
  "extensions.autoUpdate": false,
  "files.autoSave": "off",
  "breadcrumbs.enabled": false,
  "breadcrumbs.filePath": "last",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.shellIntegration.history": 0,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontFamily": " NFM",
  "vim.cursorStylePerMode.insert": "line",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<CR>"],
      "commands": [":nohlsearch"]
    },
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    }
  ],
  "zenMode.hideStatusBar": false,
  "zenMode.hideLineNumbers": false,
  "editor.occurrencesHighlight": "off",
  "editor.tabCompletion": "off",
  "editor.formatOnPaste": true,
  "editor.accessibilitySupport": "off",
  "editor.formatOnSave": true,
  "editor.fontFamily": "'Iosevka Term SS04 Extended'",
  "editor.fontWeight": "bold",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.tabSize": 2,
  "editor.cursorStyle": "block",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.wordWrap": "on",
  "editor.cursorBlinking": "solid",
  "editor.lineNumbers": "relative",
  "editor.glyphMargin": true,
  "editor.lightbulb.enabled": "off",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.cursorSurroundingLines": 999,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.limit.value": 3,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "editor.stickyScroll.enabled": false,
  "editor.renderLineHighlight": "none",
  "workbench.editor.decorations.colors": false,
  "window.titleBarStyle": "custom",
  "window.commandCenter": false,
  "workbench.startupEditor": "none",
  "workbench.editor.highlightModifiedTabs": false,
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#838282",
    "editorCursor.background": "#000"
  },
  "window.density.editorTabHeight": "default",
  "git.openRepositoryInParentFolders": "never",
  "scm.diffDecorations": "none",
  "editor.matchBrackets": "near",
  // "editor.suggest.selectionMode": "never",
  "workbench.colorTheme": "One Dark Theme",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tailwindCSS.colorDecorators": false,
  "editor.inlineSuggest.enabled": false,
  "codeium.enableConfig": {
    "*": true,
    "markdown": true,
    "glsl": true
  },
  "editor.minimap.autohide": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.sectionHeaderFontSize": 4,
  "css.lint.unknownAtRules": "ignore",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "One Dark italic",
        "scope": [
          "comment",
          "entity.other.attribute-name",
          "keyword",
          "markup.underline.link",
          "storage.modifier",
          "storage.type",
          "string.url",
          "variable.language.super",
          "variable.language.this"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "One Dark italic reset",
        "scope": [
          "keyword.operator",
          "keyword.other.type",
          "storage.modifier.import",
          "storage.modifier.package",
          "storage.type.built-in",
          "storage.type.function.arrow",
          "storage.type.generic",
          "storage.type.java",
          "storage.type.primitive"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "breadcrumbs.icons": false,
  "workbench.iconTheme": "vscode-icons"
}
