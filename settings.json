{
  "workbench.startupEditor": "newUntitledFile",
  "extensions.autoUpdate": false,
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.customMenuBarAltFocus": false,
  "editor.fontSize": 16,
  "files.autoSave": "off",
  "editor.wordWrap": "on",
  "liveServer.settings.donotShowInfoMsg": true,
  "oneDarkPro.editorTheme": "nord",
  "workbench.colorTheme": "GitHub Dark",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.cursorStyle": "block",
  "editor.cursorBlinking": "solid",
  "files.trimFinalNewlines": true,
  "editor.acceptSuggestionOnCommitCharacter": false, // I hate this fucking setting, finally found a way to turn this bitch off
  //carbon.sh starts
  "carbon.backgroundColor": "rgba(0,0,0,0)",
  "carbon.theme": "seti",
  "carbon.dropShadow": true,
  "carbon.windowControls": true,
  "carbon.autoAdjustWidth": true,
  "carbon.paddingVertical": 24,
  "carbon.paddingHorizontal": 16,
  "carbon.lineNumbers": false,
  "carbon.fontFamily": "Hack",
  "carbon.fontSize": 13,
  // carbon.sh ends
  "workbench.iconTheme": "vs-seti"
}
