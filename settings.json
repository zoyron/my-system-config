{
  "security.workspace.trust.untrustedFiles": "open",
  "extensions.autoUpdate": false,
  "files.autoSave": "off",
  "breadcrumbs.enabled": false,
  "breadcrumbs.icons": false,
  "breadcrumbs.filePath": "last",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.letterSpacing": 0.5,
  "terminal.integrated.shellIntegration.history": 0,
  "terminal.integrated.fontSize": 14,
  "vim.cursorStylePerMode.insert": "line",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<CR>"
      ],
      "commands": [
        ":nohlsearch"
      ]
    }
  ],
  "zenMode.hideStatusBar": false,
  "zenMode.hideLineNumbers": false,
  "editor.occurrencesHighlight": "off",
  "editor.tabCompletion": "off",
  "editor.formatOnPaste": true,
  "editor.accessibilitySupport": "off",
  "editor.formatOnSave": true,
  "editor.fontFamily": "'FiraCode NF'",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.tabSize": 2,
  "editor.cursorStyle": "block",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.wordWrap": "on",
  "editor.cursorBlinking": "solid",
  "editor.lineNumbers": "relative",
  "editor.glyphMargin": false,
  "editor.letterSpacing": 0.25,
  "editor.lightbulb.enabled": "off",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.cursorSurroundingLines": 999,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.iconTheme": "vscode-icons",
  "workbench.editor.limit.value": 3,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.colorCustomizations": {
    "terminal.foreground": "#cad2d6"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorTheme": "Nord",
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  "workbench.sideBar.location": "right",
  "editor.stickyScroll.enabled": false,
  "editor.renderLineHighlight": "none",
  "window.title": "${dirty}${separator}${activeEditorShort}",
  "window.commandCenter": false,
  "workbench.activityBar.location": "hidden",
}
